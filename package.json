    "@testing-library/jest-dom": "^5.16.4",
    "axios": "^0.26.1",
    "intl": "^1.2.5",
    "np": "^7.6.1",
    "react": "17.0.2",
    "react-native": "0.67.4",
    "react-native-blob-util": "^0.15.0",
    "react-native-dotenv": "^3.3.1",
    "react-native-fast-image": "^8.5.11",
